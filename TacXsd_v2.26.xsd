         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="FssLicenseType">
      <xsd:sequence>
         <xsd:element name="LicenseNumber" maxOccurs="1" minOccurs="1" type="xsd:string"/>
         <xsd:element name="LicenseDateIssued" maxOccurs="1" minOccurs="1" type="xsd:date"/>
         <xsd:element name="LicenseDateExpiration" maxOccurs="1" minOccurs="0" type="xsd:date"/>
         <xsd:element name="LicenseIssuedBy" maxOccurs="1" minOccurs="1" type="xsd:string"/>
         <xsd:element name="DecisionNumber" maxOccurs="1" minOccurs="1" type="xsd:string"/>
         <xsd:element name="DecisionDate" maxOccurs="1" minOccurs="1" type="xsd:date"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ResponsiblePersonType">
      <xsd:sequence>
         <xsd:element name="ResponsiblePersonNameFirst" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Имя</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="35"/>
                  <xsd:minLength value="1"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="ResponsiblePersonNameMiddle" maxOccurs="1" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Отчество</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="35"/>
                  <xsd:minLength value="1"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="ResponsiblePersonNameLast" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Фамилия</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="35"/>
                  <xsd:minLength value="1"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="ResponsiblePersonPosition" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
        				Должность руководителя(ответственного лица) на
        				которого оформляется карта
        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ResponsiblePersonReason" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
        				Номер и дата приказа назначения на должность
        				(руководителя) ответственного лица
        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MessageInfoType">
      <xsd:sequence>
         <xsd:element name="MessageId" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>идентификатор сообщения от поставщика сообщения (например UUID)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MessageDate" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>дата сообщения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MessageOwn" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>поставщик сообщения</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:pattern value="[A-Z]{5}"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="MessageRecipient" maxOccurs="1" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>получатель сообщения</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:pattern value="[A-Z]{5}"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="ProtocolVersion" maxOccurs="1" minOccurs="0">
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="10"/>
                  <xsd:minLength value="1"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MessageConfirmationType">
      <xsd:sequence>
         <xsd:element name="MessageInfo" maxOccurs="1" minOccurs="1" type="MessageInfoType">
            <xsd:annotation>
               <xsd:documentation>пойманное сообщение, если MessageRecipient не был заполнен в ответе он заполняется всегда кем по факту получено</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReceivedDate" maxOccurs="1" minOccurs="1" type="xsd:dateTime">
            <xsd:annotation>
               <xsd:documentation>дата получения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ErrorInfo" maxOccurs="unbounded" minOccurs="0" type="ErrorInfoType">
            <xsd:annotation>
               <xsd:documentation>Сообщение об ошибке</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Order_v3Type">
      <xsd:annotation>
         <xsd:documentation>Заявление на выдачу карт тахографа</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="OrderNumber" maxOccurs="1" minOccurs="1" type="OrderNumber_v3Type">
            <xsd:annotation>
               <xsd:documentation>
        				Номер заявления
        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderDate" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>
        				Дата подачи заявления
        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderCardType" maxOccurs="1" minOccurs="1" type="OrderCard_v3Type">
            <xsd:annotation>
               <xsd:documentation>Тип карты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CardInfo" maxOccurs="1" minOccurs="1" type="CardInfo_v3Type"/>
         <xsd:element name="OrderReason" maxOccurs="1" minOccurs="1">
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="create"/>
                  <xsd:enumeration value="change"/>
                  <xsd:enumeration value="refresh"/>
                  <xsd:enumeration value="newpd"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:choice>
            <xsd:element name="RuDriverOrder" maxOccurs="1" minOccurs="0"
               type="RuDriverOrder_v3Type">
               <xsd:annotation>
                  <xsd:documentation>Данные о водителе
        				</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RuCompanyOrder" maxOccurs="1" minOccurs="0"
               type="RuCompanyOrder_v3Type">
               <xsd:annotation>
                  <xsd:documentation>Данные о предприятии</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RuWorkshopOrder" maxOccurs="1" minOccurs="0"
               type="RuWorkshopOrder_v3Type">
               <xsd:annotation>
                  <xsd:documentation>Данные о мастерской</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RuSupervisorGibddOrder" maxOccurs="1" minOccurs="0"
               type="RuSupervisorOrder_v3Type"/>
            <xsd:element name="RuFsntOrder" maxOccurs="1" minOccurs="0"
               type="RuSupervisorOrder_v3Type"/>
            <xsd:element name="EstrDriverOrder" maxOccurs="1" minOccurs="0"
               type="EstrDriverOrder_v3Type"/>
            <xsd:element name="EstrCompanyOrder" maxOccurs="1" minOccurs="0"
               type="EstrCompanyOrder_v3Type"/>
            <xsd:element name="EstrWorkshopOrder" maxOccurs="1" minOccurs="0"
               type="EstrWorkshopOrder_v3Type"/>
            <xsd:element name="EstrSupervisorOrder" maxOccurs="1" minOccurs="0"
               type="EstrSupervisorOrder_v3Type"/>
         </xsd:choice>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Attachments_v3Type">
      <xsd:annotation>
         <xsd:documentation>Приложение к заявлению</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
