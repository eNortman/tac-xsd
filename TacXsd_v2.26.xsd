         </xsd:element>
         <xsd:element name="CardAddress" maxOccurs="1" minOccurs="1" type="RuCardAddress_v3Type"/>
         <xsd:element name="OrgInfo" maxOccurs="1" minOccurs="1" type="RuOrgInfoNoInn_v3Type"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="EstrOrgInfo_v3Type">
      <xsd:annotation>
         <xsd:documentation>Информация об организации</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="OrgNameRu" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="500"/>
                  <xsd:pattern value="[^\s]+\s[^\s].+"/>
                  <xsd:minLength value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="OrgNameEn" maxOccurs="1" minOccurs="1">
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="500"/>
                  <xsd:minLength value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="INN" maxOccurs="1" minOccurs="1" type="InnType">
            <xsd:annotation>
               <xsd:documentation>ИНН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OGRN" maxOccurs="1" minOccurs="0" type="OgrnType"/>
         <xsd:element name="KPP" maxOccurs="1" minOccurs="0">
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:pattern value="\d{9}"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="AddressPost" maxOccurs="1" minOccurs="1" type="EstrOrgAddress_v3Type">
            <xsd:annotation>
               <xsd:documentation>
        				Почтовый адрес
        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrgShortNameRu" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
        				Сокращенное наименование
        			</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
