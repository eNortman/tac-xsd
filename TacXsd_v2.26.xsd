         <xsd:element name="ResponsiblePersonNameLastEn" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Фамилия</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="35"/>
                  <xsd:minLength value="1"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:simpleType name="OksmAlpha3">
      <xsd:restriction base="xsd:string">
         <xsd:minLength value="3"/>
         <xsd:maxLength value="3"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="CardInfo_v3Type">
      <xsd:annotation>
         <xsd:documentation>Информация о карте тахографа</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CardNumber" maxOccurs="1" minOccurs="1" type="CardNumber_v3Type">
            <xsd:annotation>
               <xsd:documentation>Номер карты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueDate" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Дата начала действия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Expiration" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Дата окончания действия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertInfo" maxOccurs="1" minOccurs="0" type="CertInfo_v3Type">
            <xsd:annotation>
               <xsd:documentation>Информация о сертификате</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SKZI" maxOccurs="1" minOccurs="0">
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:pattern value="\d{3}-\d{7}"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ErrorInfoType">
      <xsd:annotation>
         <xsd:documentation>Информация об ошибке</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ErrorCode" maxOccurs="1" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Код ошибки</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ErrorMessage" maxOccurs="1" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Сообщение об ошибке</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PingMessageType">
      <xsd:annotation>
         <xsd:documentation>Диагностическое сообщение</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PingData" maxOccurs="1" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Диагностические данные (необязательное)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="CancelMessageType">
      <xsd:annotation>
         <xsd:documentation>Отзыв пакета со стороны ОВК
Отправляется из ОВК производителю и может 
быть обработано только до того, как пакет передан в производство</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CancelReason" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Причина отказа</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="CertInfo_v3Type">
      <xsd:annotation>
         <xsd:documentation>Информация о сертификате</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CertSerialNumber" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Serial number</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertIssuer" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Issuer</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertValidFrom" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Valid From</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertValidTo" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Valid To</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertSubject" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Subject</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertThumbprint" maxOccurs="1" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Thumbprint</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RuOrgInfoNoInn_v3Type">
      <xsd:annotation>
         <xsd:documentation>Информация об организации</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="OrgNameRu" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="500"/>
                  <xsd:pattern value="[^\s]+\s[^\s].+"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="OGRN" maxOccurs="1" minOccurs="0" type="OgrnType"/>
         <xsd:element name="AddressPost" maxOccurs="1" minOccurs="1" type="RuOrgAddress_v3Type">
            <xsd:annotation>
               <xsd:documentation>
        				Почтовый адрес
        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrgShortNameRu" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
        				Сокращенное наименование
        			</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="40"/>
                  <xsd:pattern value="[^\s]+\s[^\s].+"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="AddressLegal" maxOccurs="1" minOccurs="1" type="RuOrgAddress_v3Type">
            <xsd:annotation>
               <xsd:documentation>
        				Юридический адрес
        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="KPP" maxOccurs="1" minOccurs="0">
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:pattern value="\d{9}"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="INN" maxOccurs="1" minOccurs="0" type="InnType">
            <xsd:annotation>
               <xsd:documentation>ИНН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrgFace" maxOccurs="1" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>поле "в лице"</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="xsd:string">
                  <xsd:minLength value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MessageConfirmationErrorType">
      <xsd:annotation>
         <xsd:documentation>Отправлять в случае ошибки в конфирмэйшне</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="MessageInfo" maxOccurs="1" minOccurs="1" type="MessageInfoType">
            <xsd:annotation>
               <xsd:documentation>пойманное сообщение, если MessageRecipient не был заполнен в ответе он заполняется всегда кем по факту получено</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReceivedDate" maxOccurs="1" minOccurs="1" type="xsd:dateTime">
            <xsd:annotation>
               <xsd:documentation>дата получения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ErrorInfo" maxOccurs="unbounded" minOccurs="0" type="ErrorInfoType">
            <xsd:annotation>
               <xsd:documentation>Сообщение об ошибке</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="InvalideMessageType">
      <xsd:annotation>
         <xsd:documentation>В случае получения невалидного сообщения вернуть сообщение с текстом ошибки</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="InvalideMessage" type="ErrorMessageType"/>
         <xsd:element name="ErrorInfo" maxOccurs="unbounded" minOccurs="0" type="ErrorInfoType">
            <xsd:annotation>
               <xsd:documentation>Сообщение об ошибке</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:simpleType name="ErrorMessageType">
      <xsd:restriction base="xsd:string"/>
   </xsd:simpleType>
</xsd:schema>
