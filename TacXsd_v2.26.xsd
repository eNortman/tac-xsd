      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RuDriverOrder_v3Type">
      <xsd:annotation>
         <xsd:documentation>Данные, если заявитель частное лицо</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Photo" maxOccurs="1" minOccurs="1" type="Attachments_v3Type">
            <xsd:annotation>
               <xsd:documentation>Фотография</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DriverLicense" maxOccurs="1" minOccurs="1" type="DriverLicense_v3Type">
            <xsd:annotation>
               <xsd:documentation>Данные водительского удостоверения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PrevCardNumber" maxOccurs="1" minOccurs="0" type="CardNumber_v3Type">
            <xsd:annotation>
               <xsd:documentation>Номер ранее выданной карты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PersonInfo" maxOccurs="1" minOccurs="1" type="RuPersonInfo_v3Type"/>
         <xsd:element name="Sign" maxOccurs="1" minOccurs="1" type="Attachments_v3Type">
            <xsd:annotation>
               <xsd:documentation> Изображение подписи </xsd:documentation>
