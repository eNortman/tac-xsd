                  <xsd:enumeration value="appOvkPacked"/>
                  <xsd:enumeration value="appOvkReceivedFromDs"/>
                  <xsd:enumeration value="appOvkRejected"/>
                  <xsd:enumeration value="appOvkSentToFbu"/>
               </xsd:restriction>
